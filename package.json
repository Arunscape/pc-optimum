{
  "name": "pcscrapper",
  "version": "0.1.1",
  "description": "Get PDF from pcoptimum.ca",
  "main": "index.js",
  "scripts": {
    "compile": "tsc",
    "clean": "rm -R -f build",
    "build": "export PUPPETEER_SKIP_CHROMIUM_DOWNLOAD=true && npm install --production && npm install typescript && tsc && npm uninstall typescript",
    "package": "npm run build && zip -r lambda.zip node_modules && zip -ju lambda.zip build/*.js",
    "deploy": "npm run package"
  },
  "author": "",
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "https://github.com/aagavin/pc-optimum.git"
  },
  "devDependencies": {
    "@types/node": "^10.5.2",
    "@types/nodemailer": "^4.6.2",
    "@types/puppeteer": "^1.3.2"
  },
  "dependencies": {
    "nodemailer": "^4.6.7",
    "puppeteer-lambda": "1.0.12"
  }
}
